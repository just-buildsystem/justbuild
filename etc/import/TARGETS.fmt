{ "fmt-lib":
  { "type": ["@", "rules", "CC", "library"]
  , "name": ["fmt"]
  , "srcs": ["src/format.cc", "src/os.cc"]
  , "hdrs": [["./", "include/fmt", "hdrs"]]
  }
, "fmt":
  { "type": "export"
  , "target": "fmt-lib"
  , "flexible_config":
    [ "OS"
    , "ARCH"
    , "TARGET_ARCH"
    , "COMPILER_FAMILY"
    , "DEBUG"
    , "CXX"
    , "CXXFLAGS"
    , "AR"
    , "ENV"
    ]
  }
}
