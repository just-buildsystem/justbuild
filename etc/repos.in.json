      , "bazel_remote_apis": "bazel_remote_apis"
      , "libgit2": "com_github_libgit2_libgit2"
      , "protoc": "protobuf"
      }
    }
  , "lint":
    { "repository": {"type": "file", "path": "lint"}
    , "bindings":
      { "rules": "rules-just"
      , "src": "just"
      , "tests": "just tests"
      , "clang": "clang-18"
      }
    }
  , "just-distfiles":
